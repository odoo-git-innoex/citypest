<odoo>
    <record id="view_move_form_inherit_do_lpo" model="ir.ui.view">
        <field name="name">account.move.form.do.lpo</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='delivery_date']" position="after">
                <field name="do_number"/>
                <field name="lpo_number"/>
            </xpath>
        </field>
    </record>
</odoo>
